entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-25_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[agent]
expertMode = true
integrations = ["github:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "run"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "monitor"

[[workflows.workflow]]
name = "run"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python core.py"

[[workflows.workflow]]
name = "monitor"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python test.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Main"
mode = "parallel"
author = 640404

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"

[[ports]]
localPort = 8080
externalPort = 80
